import java.util.Scanner;

public class Main{

    public static void main(String[] args) {
    
	    Scanner scanner = new Scanner(System.in);
        int N1 = scanner.nextInt();
    
	    int N2 = scanner.nextInt();
        scanner.close();

        int count = 0;

        int number = 1;

        while (count < N1) {

            int result = 3 * number + 2;
            if (result % N2 != 0) {

                System.out.println(result);
                count++;

            }
            number++;
        }
    }
}
